; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="ByeBack"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://icon.svg"

[autoload]

BG="*res://globals/background.tscn"
MUSIC="*res://globals/bg_music.tscn"
SFX="*res://globals/bg_sfx.tscn"
CHOICES="*res://globals/choices.tscn"
DEBUG="*res://globals/debug.tscn"
FG="*res://globals/foreground.tscn"
GAME="*res://globals/game.tscn"
PALETTE="*res://globals/palette.gd"
STORY="*res://globals/story.tscn"

[display]

window/size/viewport_width=1088
window/size/viewport_height=1920
window/size/window_width_override=544
window/size/window_height_override=960
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/handheld/orientation=1

[gui]

theme/custom="res://resources/themes/byeback.tres"

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
